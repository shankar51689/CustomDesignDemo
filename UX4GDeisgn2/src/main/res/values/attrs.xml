        <attr name="circleX" format="dimension"/>
        <attr name="circleY" format="dimension"/>
        <attr name="isDraggable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CircularImageView">
        <attr name="borderWidth" format="dimension"/>
        <attr name="borderColor" format="color"/>
    </declare-styleable>

